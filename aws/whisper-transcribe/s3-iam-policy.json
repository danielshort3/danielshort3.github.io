{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "WhisperUploadsObjectAccess",
      "Effect": "Allow",
      "Action": ["s3:PutObject", "s3:GetObject", "s3:DeleteObject"],
      "Resource": "arn:aws:s3:::danielshort-whisper-transcribe-uploads-886623862678-us-east-2/whisper-uploads/*"
    },
    {
      "Sid": "WhisperUploadsBucketList",
      "Effect": "Allow",
      "Action": ["s3:ListBucket"],
      "Resource": "arn:aws:s3:::danielshort-whisper-transcribe-uploads-886623862678-us-east-2",
      "Condition": {
        "StringLike": {
          "s3:prefix": ["whisper-uploads/*"]
        }
      }
    }
  ]
}
